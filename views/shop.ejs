<%- include('includes/head.ejs') %>
</head>
<body>
    <%- include('includes/nav.ejs') %>
    <main>
        <div class="container">
        <h1>My products</h1>
        <% if (products.length > 0) { %>
            <ul class="list-group">
            <% for (let product of products) { %>
            <div class="product">
                <li class="list-group-item"><%= product.title %></li>
            </div>
            
            <% } %>
            </ul>
        <%} else {%>
            <h4>No products found</h4>
        <% } %>
    </div>
    </main>
</body>
</html>